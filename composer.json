{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "appstract/laravel-opcache": "^4.0",
        "bezhansalleh/filament-shield": "^4.0",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "codeat3/blade-akar-icons": "^1.25",
        "codeat3/blade-clarity-icons": "^1.10",
        "codeat3/blade-codicons": "^1.34",
        "codeat3/blade-iconpark": "^1.7",
        "codeat3/blade-unicons": "^1.9",
        "davidhsianturi/blade-bootstrap-icons": "^2.1",
        "doctrine/dbal": "^4.4",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "flowframe/laravel-trend": "^0.4.0",
        "laravel-notification-channels/telegram": "^6.0",
        "laravel/framework": "^12.0",
        "laravel/socialite": "^5.23",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.4",
        "livewire/volt": "^1.7.0",
        "mallardduck/blade-lucide-icons": "^1.23",
        "marineusde/larapex-charts": "^1.5",
        "opcodesio/log-viewer": "^3.21",
        "owenvoke/blade-fontawesome": "^2.9",
        "phpoffice/phpspreadsheet": "^5.4",
        "phpoffice/phpword": "^1.4",
        "secondnetwork/blade-tabler-icons": "^3.31",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.6",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/vkontakte": "^5.1",
        "socialiteproviders/yandex": "^4.1",
        "spatie/laravel-ignition": "^2.9",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-permission": "^6.23",
        "spatie/livewire-filepond": "^1.5",
        "staudenmeir/eloquent-json-relations": "^1.14",
        "yoomoney/yookassa-sdk-php": "^3.10"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "buggregator/trap": "^1.13",
        "fakerphp/faker": "^1.23",
        "laradumps/laradumps-core": "^3.2",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "maantje/xhprof-buggregator-laravel": "^0.7.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.1",
        "sentry/sentry-laravel": "^4.18"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74,#86efac,#fca5a5,#fde047\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" \"php artisan queue:work\" \"F:\\\\Work\\\\mailpit.exe\" --names 'server,queue-listen,vite,queue-work,mailpit,tunnel'"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
